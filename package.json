{
  "name": "vcloudblock-resource",
  "version": "0.2.0",
  "description": "External resources for vcloudblock projects",
  "main": "index.js",
  "bin": {
    "gen-config": "./script/bin/gen-config.js",
    "upload-releaseInfo": "./script/bin/upload-latest-release-info.mjs",
    "upload-file": "./script/bin/upload-file.mjs",
    "i18n-extract": "./script/i18n/extract-format-message.js",
    "i18n-push": "./script/i18n/push-format-message.js",
    "i18n-update": "./script/i18n/update-translations.js"
  },
  "scripts": {
    "fetch": "rimraf external-resources download && node script/download.js --repo=vcloudblock/external-resources-v3 --plat=github",
    "start": "node ./test/start-server.js",
    "start:clear": "rimraf ../.vcloudblockData/external-resources && node ./test/start-server.js",
    "clean": "rimraf external-resources dist translation download ../.vcloudblockData/external-resources",
    "build:extension": "node script/generate-extension-page.js --url=https://vcloudblock.github.io/external-resources/",
    "build:device": "node script/generate-device-page.js --url=https://vcloudblock.github.io/external-resources/",
    "build": "rimraf dist && npm run build:extension && npm run build:device",
    "deploy": "npm run fetch && npm run build",
    "lint": "eslint ./",
    "test:update": "node test/update.js",
    "test:cnUpdate": "node test/cnUpdate.js",
    "test:multi-update": "node test/multi-update.js",
    "test:multi-server": "node test/multi-server.js",
    "test:abort-check": "node test/abortCheckUpdate.js",
    "test:abort-update": "node test/abortUpdate.js"
  },
  "author": "NETBOY",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.598.0",
    "@root/walk": "^1.1.0",
    "@transifex/api": "^7.1.1",
    "cli-color": "^2.0.4",
    "compare-versions": "^3.6.0",
    "download": "^8.0.0",
    "express": "^4.17.1",
    "extract-zip": "^2.0.1",
    "format-message": "^6.2.3",
    "format-message-parse": "^6.2.4",
    "fs-extra": "^10.1.0",
    "hash-files": "^1.1.1",
    "lodash": "^4.17.21",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "^2.7.0",
    "node-fetch-progress": "^1.0.2",
    "vcloudblock-l10n": "^3.15.0",
    "vcloudblock-parse-release-message": "0.0.1",
    "proper-lockfile": "^4.1.2",
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "copy-dir": "^1.3.0",
    "eslint": "^5.3.0",
    "eslint-config-scratch": "^5.0.0",
    "progress": "^2.0.3",
    "rimraf": "^3.0.2"
  }
}
